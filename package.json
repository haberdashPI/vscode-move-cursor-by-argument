{
	"name": "move-cursor-by-argument",
	"publisher": "haberdashPI",
	"repository": "https://github.com/haberdashPI/vscode-move-cursor-by-argument",
	"displayName": "Move Cursor by Argument",
	"description": "Move cursor from one function argument to the next",
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:move-cursor-by-argument.move-by-argument"
	],
	"browser": "./dist/web/main.js",
	"contributes": {
		"commands": [
			{
				"command": "move-cursor-by-argument.move-by-argument",
				"title": "Move by Function Argument"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "npx webpack --mode development",
		"watch": "npx webpack --watch --mode development",
		"package": "npx webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^8.7.0",
		"glob": "^7.1.6",
		"localtunnel": "^2.0.2",
		"mocha": "^9.2.0",
		"serve": "^13.0.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^5.76.0",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"ts-loader": "^9.2.6"
	}
}
